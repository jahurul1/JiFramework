{
    "name": "jahurul1/ji-framework",
    "description": "A lightweight and efficient PHP framework designed to simplify web application development. Features timezone-aware DateTimeHelper, database query builder, security tools, and comprehensive utility classes.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.3",
    "homepage": "https://github.com/jahurul1/JiFramework",
    "keywords": ["framework", "php", "datetime", "database", "security", "utilities", "timezone", "query-builder"],
    "authors": [
        {
            "name": "Jahurul Islam",
            "email": "info@jahurul.in"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "JiFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JiFramework\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
} 
